<h1>Your booking is soon confirmed</h1>

<%= simple_form_for([@piece, @transaction]) do |f| %>
  <%= f.input :start_date,
            as: :string,
            input_html: { data: { controller: "datepicker" } }  %>
  <%= f.input :end_date,
            as: :string,
            input_html: { data: { controller: "datepicker" } }  %>
  <%= f.submit %>
<% end %>
